{
    "name": "funayaki/tiny-post",
    "description": "TinyPost plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4",
        "funayaki/adminlte": "dev-feature"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "TinyPost\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TinyPost\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
